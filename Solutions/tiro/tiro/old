#include <stdio.h>
#include <stdlib.h>
int main(){
    
    int n;
    long int coord[1000][2];
    int k;
    int maxCon,ri;
    int instancia,con;
    int contador = 0;


    scanf("%d",&instancia);
    
    while(contador < instancia)
    {
        k = 0;
        maxCon = 1;
        
        scanf("%d",&n);
        while(k < n)
        {
            scanf("%ld %ld",&coord[k][0],&coord[k][1]);
            k++;
        }
        
        
        float m,m2 ;
        
        for(ri = 0 ; ri<n-1 ; ri++)
        {

            
            if (coord[ri+1][0]-coord[ri][0] == 0 )
                m = 0;
                else
                    m = (coord[ri+1][1] - coord[ri][1])*1.0/(coord[ri+1][0]- coord[ri][0]);
            
            con =  2;

            for(k = (ri+2) ; k<n ; k++)
            {

                if (coord[k][0]-coord[ri][0] == 0 )
                m2= 0;
                    else
                    m2 = (coord[k][1] - coord[ri][1])*1.0/(coord[k][0] - coord[ri][0]);
                    
                       if( m == m2)
                           con++;
                
                
            }
            if(con>maxCon)
                maxCon = con;
        }
        
        printf("%d\n",maxCon);
        contador ++;
        
        
    }
    
    
    
    return 0;
    
}

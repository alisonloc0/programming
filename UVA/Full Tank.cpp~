#include <cstdlib>
#define MAXN 1010
#define MAXM 10010
#define INF 1000000000
using namespace std;

vector< pair<int,int> >[MAXN] list;
vector<int> p;
int S,T,C;			
int cost[MAXN][20]; 

struct foo{
  int w,c,f;
  foo(int w_,int c_,int f_){
    w = w_;
    c = c_;
    f = f_;
  }
  bool operator > ( const foo& A) const{
    return A.c > c;
  }
}


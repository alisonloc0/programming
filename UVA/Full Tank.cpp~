}


int main(){
  int N,M;
  cin >> N >> M;
  for(int i=0;i<N;i++){
    int x;
    p.push_back(x);
  }
  for(int i=0;i<M;i++){
    int v,w,d;
    cin >> v >> w >> d;
    list[v].push_back(make_pair(w,d));
  }
  int k; cin >> k;
  for(int i=0;i<k;i++){
    cin >> C >> S >> T;
    for(int i=0;i<MAXN;i++){
      for(int j=0;i<20;j++){
	cost[i][j] = INF;
      }
    }
    sssp();
  }
  return 0;
}
